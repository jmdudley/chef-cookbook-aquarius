<?xml version="1.0" encoding="utf-8"?>
<AqDataSource xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Configuration>Basic</Configuration>
	<ServerType>Oracle</ServerType>
	<ConnectionString>user id=<%= @db_username %>;password=<%= @db_password %>;data source=//<%= @db_server_name %>:1521/<%= @db_name %></ConnectionString>
	<ServerName><%= @db_server_name %></ServerName>
	<DatabaseName><%= @db_name %></DatabaseName>
	<Port>1521</Port>
	<Authentication>Standard</Authentication>
	<UserName><%= @db_username %></UserName>
	<Password><%= @db_password %></Password>
</AqDataSource>